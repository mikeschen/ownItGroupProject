  <div class = "container">
    <h2 class="topMargin">Hello $user.getName()</h2>
    <h3><em>Snowboarding<em> Trick List</h3>
    <hr>
    <h4><strong><em>Flatground:</strong></em></h4>
      #foreach ($trick in $userSnowTricksFlat)
      <div class="flex-container">
      <div class="btn-group bottomMargin">
        <form class="btn btn-custom" action="/update/trick/$trick.getId()" method="get">
          <input id="trickId" name="trickId" type="hidden" value="$trick.getId()">
          <button type="submit" id="$trick.getId()" class="btn-custom"><i class="fa fa-cog"></i> Update</button>
        </form>
        <p class="btn btn-custom thePadding">Category: $trick.getCategoryName()</p>
        <p class="btn btn-custom thePadding">Name: $trick.getName() </p>
        <p class="btn btn-custom thePadding">Rating: $trick.getRatingName() </p>
        <p class="btn btn-custom thePadding">Date: $trick.getDate()</p>
        <form class="btn btn-custom" action="/snowboarding/delete/trick/$trick.getId()" method="post">
          <input id="trickId" name="trickId" type="hidden" value="$trick.getId()">
          <button type="submit" id="$trick.getId()" class="btn-custom"><i class="fa fa-trash-o fa-lg"></i> Delete Trick</button>
        </form>
      </div>
    </div>
      #end
